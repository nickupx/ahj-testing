{
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
},
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "semi": "off"
  }
}